{
  "name": "Stop Autoplay for Youtube\u2122 Extended",
  "version": "4.1.1",
  "manifest_version": 2,
  "description": "Stops autoplay on YouTube\u2122. Extended: Starts the video when you focus the tab. Don't like? Use the version without 'Extended'.",
  "icons": {
    "128": "assets/images/icon128.png",
    "16": "assets/images/icon16.png",
    "48": "assets/images/icon48.png"
  },
  "author": "Ahmet Ye≈üil",
  "content_scripts": [
    {
      "matches": [
        "http://www.youtube.com/*",
        "http://youtube.com/*",
        "https://www.youtube.com/*",
        "https://youtube.com/*"
      ],
      "css": ["dist/css/style.min.css"],
      "js": [
        "start.js",
        "dist/js/all-min.js"
      ],
      "run_at": "document_start"
    }
  ],
  "content_security_policy": "script-src 'self' https://cdn.firebase.com https://*.firebaseio.com; object-src 'self'",
  "web_accessible_resources": [
    "main.js",
    "index.html"
  ],
  "permissions":[
    "https://*/*",
    "http://*/*",
    "tabs",
    "activeTab",
    "storage"
  ],
  "minimum_chrome_version": "42"
}
